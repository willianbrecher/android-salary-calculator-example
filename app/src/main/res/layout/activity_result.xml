        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/txtSalarioBruto"
        android:layout_width="150dp"
        android:layout_height="17dp"
        android:layout_marginStart="209dp"
        android:layout_marginTop="150dp"
